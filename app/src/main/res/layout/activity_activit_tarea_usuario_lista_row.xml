<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vista.ActivityListaAdapterTecnico">

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="360dp"
        android:layout_height="169dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20pt"
                android:layout_marginTop="10dp"
                android:text="100"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/lblfecha"
                android:layout_width="156dp"
                android:layout_height="35dp"
                android:layout_marginLeft="90pt"
                android:layout_marginTop="10dp"
                android:text="TextView"
                android:textColor="@android:color/black" />
        </LinearLayout>


        <TextView
            android:id="@+id/lblDescripcion"
            android:layout_width="296dp"
            android:layout_height="35dp"
            android:layout_marginLeft="65px"
            android:layout_marginTop="5dp"
            android:shadowColor="@android:color/black"
            android:text="TextView" />

        <View
            android:id="@+id/divider"
            android:layout_width="300dp"
            android:layout_height="3dp"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="55px"
            android:layout_marginTop="3dp"
            android:background="?android:attr/listDivider"
            android:paddingLeft="15dp"
            android:paddingRight="15dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/lblAsignadA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="3dp"
                android:text="By:"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/lblNombreUsuarioAsignado"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="3dp"
                android:text="TextView"
                android:textColor="@android:color/black" />
        </LinearLayout>

        <TextView
            android:id="@+id/lblCategoria"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="3dp"
            android:text="TextView"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblProceso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="225dp"
            android:text="TextView"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>