<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vista.LoginRegistroActivity"
    >

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email"
        android:textColor="@android:color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="34dp" />

    <EditText
        android:id="@+id/txtEmail"
        android:layout_width="342dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/email"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="53dp"
        tools:ignore="MissingConstraints"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text="@string/nombre"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="119dp"
        tools:ignore="MissingConstraints"/>

    <EditText
        android:id="@+id/txtnombre"
        android:layout_width="342dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/nombre"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="139dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contrace_a"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="213dp"
        tools:ignore="MissingConstraints"/>

    <EditText
        android:id="@+id/txtContracena"
        android:layout_width="342dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/contrace_confirmar_a"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="232dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/comfirmar_contrace_a"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="291dp"
        tools:ignore="MissingConstraints"/>

    <EditText
        android:id="@+id/txtConfirmarContracena"
        android:layout_width="344dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/confirmar_contrace_a"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="310dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tpo_de_usuario"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="368dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/rdTecnico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tecnico"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="404dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/rdNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/normal"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="404dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/btnCancelar"
        android:layout_width="129dp"
        android:layout_height="wrap_content"
        android:text="@string/cancelar"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="457dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/btnRegistrar"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:text="@string/registrar"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="457dp"
        tools:ignore="MissingConstraints"/>

</android.support.constraint.ConstraintLayout>